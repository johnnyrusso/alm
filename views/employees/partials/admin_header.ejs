  <% if(cssFiles.length > 0) { %>
  <% for(let i = 0; i < cssFiles.length; i++) { %>
  <% if(cssFiles[i].substring(0, 4) !== 'http') { let cssLink = cssFiles[i] + '.css'; %>
  <link rel="stylesheet" type="text/css" href="/public/css/employees/<%= cssLink %>">
  <%} else if( cssFiles[i].substring(0, 4) === 'http' ) { let cssLink = cssFiles[i] + '.css'; %>
  <link rel="stylesheet" type="text/css" href="<%= cssLink %>">
  <% }}%>
  <% } %>

</head>

